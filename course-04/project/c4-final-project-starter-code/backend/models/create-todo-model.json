{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
		"minLength": 3
    },
    "dueDate": {
      "type": "string",
		"pattern": "^\\d{4,4}-\\d{2,2}-\\d{2,2}$"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}
